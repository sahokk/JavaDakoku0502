package utility;

import org.openqa.selenium.By;

public enum JcInformations {
	LOGIN_URL("https://z001.majorflowz.com/MFZT/2515/ZtAccount/Login"), LOGIN_ID_FIELD(id, "LoginId"),
	LOGIN_PASS_FIELD("zc_Password"), LOGIN_BUTTON_FIELD("/html/body/div[1]/div/form/div[1]/div[2]/div[3]/button"),
	DAKOKU_URL(""), DAKOKU_BUTTON_IN("\"/html/body/div[2]/div/div/div[1]/div[3]/div/div[2]/ul/li[1]/button\""),
	DAKOKU_BUTTON_OUT("/html/body/div[2]/div/div/div[1]/div[3]/div/div[2]/ul/li[2]/button");

	private By by;
	private String str;

	private JcInformations(By by, String str) {
		this.by = by;
		this.str = str;
	}

	public String getValue() {
		return this.str;
	}
}
