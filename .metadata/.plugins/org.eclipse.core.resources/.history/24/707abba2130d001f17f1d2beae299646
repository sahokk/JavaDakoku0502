package db_control;

import utility.DbColumns;

public class JcInfoControl extends DbControl {

	private static final String TABLE_NAME = "jc_info";

	public JcInfoControl() {
		super();
	}

	@Override
	protected String getInfo(DbColumns columns, String tableName) {
		return super.getInfo(columns, TABLE_NAME);
	}

	@Override
	protected void setInfo(String loginId, String loginPass, String tableName) {
		super.setInfo(loginId, loginPass, TABLE_NAME);
	}

	@Override
	protected void updateInfo(String loginId, String loginPass, String tableName) {
		super.updateInfo(loginId, loginPass, TABLE_NAME);
	}

}
