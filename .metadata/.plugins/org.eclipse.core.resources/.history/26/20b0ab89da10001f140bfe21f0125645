package web_control;

import dakoku.JcDakoku;
import login.JcLogin;
import utility.MyUrls;

public class JcWebControl extends WebControl {

	private static JcWebControl webControl = new JcWebControl();
	private static JcLogin jcLogin = new JcLogin(null);
	private static JcDakoku jcDakoku = new JcDakoku(jcLogin);

	private JcWebControl() {
		super();
		setPageUrl(MyUrls.JC_LOGIN_URL.getValue());
		accessLoginPage();
	}

	public static JcWebControl getInstance() {
		return webControl;
	}

}
