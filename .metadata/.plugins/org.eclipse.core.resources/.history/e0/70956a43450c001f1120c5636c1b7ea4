	LOGIN_PASS_FIELD("zc_Password"), LOGIN_BUTTON_FIELD("/html/body/div[1]/div/form/div[1]/div[2]/div[3]/button"),
	DAKOKU_URL(""), DAKOKU_BUTTON_IN(""), DAKOKU_BUTTON_OUT("");

	private String str;

	private JcInformations(String str) {
		this.str = str;
	}

	public String getValue() {
		return this.str;
	}
}
