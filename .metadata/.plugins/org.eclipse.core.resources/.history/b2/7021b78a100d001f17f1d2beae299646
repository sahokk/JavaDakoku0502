package dakoku;

import org.openqa.selenium.By;

import login.RLogin;
import utility.Dakokustate;

public class RDakoku extends Dakoku {
	private By dakokuListButton;

	public RDakoku(String dakokuUrl, By dakokuButtonIn, By dakokuButtonOut, By dakokuListButton) {
		super(dakokuUrl, dakokuButtonIn, dakokuButtonOut);
		this.dakokuListButton = dakokuListButton;
	}

	@Override
	public void pushDakokuListButton() {
		getDriver().findElement(dakokuListButton).click();

	}

	@Override
	public void dakoku(Dakokustate dakokustate) {
		RLogin.getInstance().login();
		super.dakoku(dakokustate);
	}

}
