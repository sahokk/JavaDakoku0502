package login;

import org.openqa.selenium.net.Urls;

import utility.JcInformations;

public class JcLogin extends Login {
	private static Login jcLogin = new JcLogin((Urls.LOGIN_URL.getValue(), JcInformations.LOGIN_ID_FIELD.getValue(),
				JcInformations.LOGIN_PASS_FIELD.getValue(), JcInformations.LOGIN_BUTTON_FIELD.getValue());

	public JcLogin(String loginUrl, String loginIdField, String loginPassField, String loginButton) {
		super(loginUrl, loginIdField, loginPassField, loginButton);
	}

	public static Login getInstance() {
		return jcLogin;
	}

}
