package utility;

public enum RInformations {
	LOGIN_URL("https://r-system1.com/Login/Index/0"), LOGIN_ID_FIELD("TxtUserId"), LOGIN_PASS_FIELD("TxtPassword"),
	LOGIN_BUTTON_FIELD("/html/body/div/div/div/form/div[5]/div[1]/button"),
	DAKOKU_URL("https://r-system1.com/TimeCard"), DAKOKU_LIST_BUTTON("menuStamp"), DAKOKU_BUTTON_IN("btnStartTime"),
	DAKOKU_BUTTON_OUT("btnEndTime");

	private String str;

	private RInformations(String str) {
		this.str = str;
	}

	public String getValue() {
		return this.str;
	}
}
